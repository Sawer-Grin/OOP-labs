<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="517.0" prefWidth="712.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mainapp.CreatingOverviewController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
            <Menu mnemonicParsing="false" text="New ">
              <items>
                  <MenuItem mnemonicParsing="false" onAction="#createFreshman" text="Freshman" />
                  <MenuItem mnemonicParsing="false" onAction="#createJunior" text="Junior" />
                  <MenuItem mnemonicParsing="false" onAction="#createSenior" text="Senior" />
                  <MenuItem mnemonicParsing="false" onAction="#createSophomore" text="Sophomore" />
                  <MenuItem mnemonicParsing="false" onAction="#createDoctor" text="Doctor" />
                  <MenuItem mnemonicParsing="false" onAction="#createMaster" text="Master" />
              </items>
            </Menu>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#readTextFile" text="Open" />
                  <MenuItem mnemonicParsing="false" onAction="#saveTextFile" text="Save" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
